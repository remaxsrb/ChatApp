# .nginx.env

# Backend user service URL (for reverse proxy)
USER_SERVICE_URL=http://user-service:9090

# SSL certificate and key paths (adjust based on your actual folder structure)
SSL_CERT_PATH=/etc/nginx/certs/server-cert.pem
SSL_KEY_PATH=/etc/nginx/certs/server-key.pem

# Server settings
NGINX_HOST=user-service
NGINX_PORT=443
